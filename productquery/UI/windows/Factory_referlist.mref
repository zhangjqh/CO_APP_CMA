                <input id="checkbox0" bindfield="checked" type="checkbox" checked="checked" disabled="disabled"/>
                <label id="label4" bindfield="factorycode">label</label>
                <label id="label3" bindfield="factoryname" type="multiline">多行文本</label> 
            </div> 
        </listView> 
    </div> 
</window>
