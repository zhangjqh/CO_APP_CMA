<?xml version="1.0" encoding="UTF-8"?>

<window xmlns:web="http://www.yonyou.com/uapmobile/dsl" id="Factory_referlist" context="ref" controller="Factory_referlistController" namespace="com.ump.productquery">
    <import ref="Factory_referlist.css" type="css"/>
    <action id="onCancel" resultcode="0" method="UMView.close"/>
    <action id="close" resultcode="0" method="UMView.close"/>
    <div id="viewPage0" onload="this.load_reflist()">
        <navigatorbar id="navigatorbar0">
